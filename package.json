  "lingui": {
    "catalogs": [
      {
        "path": "<rootDir>/src/language/locales/{locale}/catalog",
        "include": [
          "<rootDir>/src"
        ],
        "exclude": [
          "**/__tests__/**"
        ]
      }
    ],
    "format": "minimal",
    "sourceLocale": "en",
