{
  "name": "@gooddollar/gooddapp",
  "version": "2.8.0",
  "engines": {
    "node": ">=16.x <17",
    "npm": "8.x"
  },
  "scripts": {
    "web": "npm run lingui:compile && react-app-rewired start",
    "android": "npm run lingui:compile && export ENVFILE=.env;npm run copy:gdfont && npm run animation:assets && react-native run-android",
    "ios": "npm run lingui:compile && export ENVFILE=.env;npm run animation:assets && react-native run-ios",
    "start:native": "react-native start",
    "start": "serve -s -l $PORT build",
    "analyze:sme": "source-map-explorer 'build/static/js/*.js'",
    "analyze": "webpack-bundle-analyzer 'build/stats.json'",
    "start:local": "npm link @gooddollar/goodcontracts && npm start",
    "start-clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm run start:native -- --reset-cache",
    "link": "npm link @gooddollar/goodcontracts",
    "web:local": "npm run link && npm run web",
    "build": "npm run lingui:compile && react-app-rewired build --analyze",
    "animation:assets": "node ./animationAssets.js",
    "copy:gdfont": "cp src/assets/fonts/*.ttf android/app/src/main/assets/fonts/",
    "test": "npm run lingui:compile && npm run test:web && npm run test:native",
    "test:setup": "./scripts/blockchainTestSetup.sh",
    "test:web-watch": "npm run link && node scripts/test.js --watch --config \"./config/web.jest.config.js\"",
    "test:web": "REACT_APP_ENV=test react-app-rewired test --forceExit",
    "test:local": "REACT_APP_ENV=test npm run lingui:compile && npm run test:web",
    "test:native": "npm run test:ios && npm run test:android",
    "test:ios": "npm run animation:assets && ENVFILE=.env.test detox build -c ios.sim.debug && detox test -c ios.sim.debug",
    "test:android": "npm run animation:assets && ENVFILE=.env.test detox build -c android.emu.debug && detox test -c android.emu.debug",
    "coverage": "npm run lingui:compile && REACT_APP_ENV=test react-app-rewired test --coverage --forceExit",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "bundlesize:check": "bundlesize",
    "flow": "flow",
    "backend": "npm run start --prefix node_modules/@gooddollar/goodcontracts/",
    "lint": "eslint \"src/**/*.js\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "docs": "node scripts/docs.js",
    "web:serveo": "export REACT_APP_WEB3_RPC=https://gdchain.serveo.net;export REACT_APP_SERVER_URL=https://gdserver.serveo.net;npm run web:local",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "add-locale": "lingui add-locale",
    "lingui:extract": "lingui extract --clean",
    "lingui:compile": "lingui compile && node scripts/i18n.js",
    "lingui": "npm run lingui:extract --clean && npm run lingui:compile",
    "ganache": "npx ganache -D",
    "clearcfcache": "node cloudflare-purge.js",
    "postbuild": "npm run clearcfcache"
  },
